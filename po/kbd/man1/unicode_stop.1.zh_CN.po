#. type: Plain text
#: raw/kbd/man1/unicode_stop.1:5
msgid "unicode_stop - revert keyboard and console from unicode mode"
msgstr "unicode_stop - 将键盘和控制台从 unicode 模式回退"

#. type: SH
#: raw/kbd/man1/unicode_stop.1:5
#, no-wrap
